<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.carRental.provider.mapper.CarMapper">

    <resultMap id="Car" type="com.carRental.provider.model.Car" >
        <result column="id" property="id" />
        <result column="model_name" property="modelName" />
        <result column="displacement" property="displacement" />
        <result column="rent_price" property="rentPrice" />
        <result column="age" property="age" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="is_delete" property="isDelete" />
    </resultMap>


</mapper>
